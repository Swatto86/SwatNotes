{
  "name": "swatnotes-ui",
  "version": "1.0.8",
  "description": "SwatNotes UI",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "wdio run wdio.conf.cjs",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,json,css}\""
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-dialog": "^2.0.0",
    "@tauri-apps/plugin-fs": "^2.0.0",
    "@tauri-apps/plugin-global-shortcut": "^2.0.0",
    "@tauri-apps/plugin-notification": "^2.0.0",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "quill": "^2.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@tauri-apps/cli": "^2.0.0",
    "@types/node": "^20.11.0",
    "@types/quill": "^2.0.14",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "@wdio/cli": "^9.23.3",
    "@wdio/local-runner": "^9.23.3",
    "@wdio/mocha-framework": "^7.33.0",
    "@wdio/spec-reporter": "^9.23.3",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.14",
    "globals": "^15.0.0",
    "happy-dom": "^20.3.1",
    "postcss": "^8.4.49",
    "prettier": "^3.2.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.17",
    "webdriverio": "^9.23.3"
  }
}
